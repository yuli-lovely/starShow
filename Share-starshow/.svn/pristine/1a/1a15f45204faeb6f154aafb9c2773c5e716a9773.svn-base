 body{
    margin: 0;padding: 0;
    -webkit-font-smoothing: antialiased;
   /* font-family: lanhei;*/
    color: #000;
}
html{
    font-family: STHeiti,'Microsoft YaHei',Helvetica,Arial,sans-serif;
}
